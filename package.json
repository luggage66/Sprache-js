{
  "name": "sprache",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index",
    "build": "tsc",
    "clean": "rimraf dist documentation coverage",
    "test": "jest",
    "test:ci": "jest --ci --collectCoverage --reporters=\"default\" --reporters=\"jest-junit\"",
    "build:docs": "typedoc --out documentation src",
    "semantic-release": "semantic-release"
  },
  "homepage": "https://github.com/luggage66/Sprache-js",
  "bugs": {
    "url": "https://github.com/luggage66/Sprache-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luggage66/Sprache-js.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "coveralls": "^3.0.3",
    "jest": "^26.6.1",
    "jest-extended": "^0.11.1",
    "jest-junit": "^12.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.2",
    "source-map-support": "^0.5.12",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "make-error": "^1.3.5"
  }
}
